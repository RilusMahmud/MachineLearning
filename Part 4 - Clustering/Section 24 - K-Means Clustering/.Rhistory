setwd("~/Machine Learning A-Z CodeGround/Part 4 - Clustering/Section 24 - K-Means Clustering")
# Importing the mall dataset
dataset <- read.csv('Mall_Customers.csv')
View(dataset)
X <- dataset[4,5]
X <- dataset[4:5]
View(X)
set.seed(6)
wcss <- vector()
for (i in 1:10)
wcss[i] <- sum(kmeans(X, i)$withinss)
plot(1:10, wcss, type = 'b',
main = paste('Cluster of clients'),
xlab = 'Number of clients',
ylab = 'WCSS')
set.seed(29)
kmeans <- kmeans(X, 5, iter.max = 300, nstart = 10)
View(kmeans)
library(cluster)
clusplot(X,
kmeans$cluster,
lines = 0,
shade = TRUE,
color = TRUE,
labels = 2,
plotchar = FALSE,
span = TRUE,
main = paste('Cluster of clients'),
xlab = 'Annula income',
ylab = 'Spending score')
